{
  "name": "test-series",
  "version": "1.0.0",
  "description": "test-series-application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js | pino-pretty",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@types/mssql": "^9.1.5",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^4.13.8",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.2",
    "pdf-poppler": "^0.2.1",
    "pino": "^7.8.1",
    "sharp": "^0.32.6",
    "swagger-express-middleware": "^4.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/body-parser": "^1.19.4",
    "@types/chai": "^4.3.0",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-express-middleware": "^1.0.14",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.3",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.5",
    "supertest": "^6.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
